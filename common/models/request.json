{
  "name": "request",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "time": {
      "type": "date",
      "required": true
    },
    "status": {
      "type": "string",
      "default": "active"
    }
  },
  "validations": [],
  "relations": {
    "member": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "memberId"
    },
    "join": {
      "type": "hasOne",
      "model": "Join",
      "foreignKey": "requestId"
    },
    "destination": {
      "type": "belongsTo",
      "model": "destination",
      "foreignKey": "destination_name"
    },
    "pickup": {
      "type": "belongsTo",
      "model": "pickup",
      "foreignKey": "pickup_name"
    },
    "matchedSeat": {
      "type": "hasMany",
      "model": "MatchedSeat",
      "foreignKey": "requestId"
    },
    "pendingSeat": {
      "type": "hasMany",
      "model": "PendingSeat",
      "foreignKey": "requestId"
    },
    "requestQueue": {
      "type": "hasMany",
      "model": "RequestQueue",
      "foreignKey": "requestId"
    }
  },
  "acls": [],
  "methods": {}
}
